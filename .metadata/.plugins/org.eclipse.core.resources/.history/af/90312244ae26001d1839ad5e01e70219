package com.enums;

public class EnumWith_Values_Ordinals {

	public static void main(String[] args) {
		
		Month[] month = Month.values();
		
		for(int i=0; i<month.length;) {
			if(i== month.length) {
				break;
			}
			
			System.out.println(month[i]+" ---> "+month[++i].ordinal());
		}
		
	}
	
}
